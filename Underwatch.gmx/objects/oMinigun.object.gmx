          <argument>
            <kind>1</kind>
            <string>///Positioning and Actions

RightGunPosition();

///GenericShooting();

///Shooting
var Len = sprite_width;
var Dir = image_angle + Flip;

SecondsSinceShot = SecondsSinceShot + 1/room_speed;

if mouse_check_button(mb_left) {
    
    if SecondsSinceShot &gt; SecondsBetweenShots &amp;&amp; Ammo &gt; 0 {
        image_speed = 0.4;
        var Bullet = instance_create(x+lengthdir_x(Len, Dir), y+lengthdir_y(Len, Dir), BulletType);
        Bullet.speed = BulletSpeed;
        Bullet.direction = image_angle + random_range(-Spray,Spray);
        Bullet.image_angle = Bullet.direction;
        Bullet.Owner = id;
        var Bang = instance_create(x + lengthdir_x(Len, Dir), y + lengthdir_y(Len, Dir), MuzzleType);
        Bang.image_angle = image_angle;
        Ammo = Ammo - 1;
        SecondsSinceShot = 0;
        
        //ONLY MINIGUN GETS SHELL CASINGS ATM 
        if global.PlayerType == 4 {
            var Casing = instance_create(x+lengthdir_x(Len/4, Dir), y+lengthdir_y(Len/4, Dir), oShellCasing);
            Casing.Owner = id;
            Casing.direction = image_angle + 250 + random_range(-20,20);
        }
    }
    
} else {
    image_speed = 0;
}

if Ammo &lt;= 0 {
    Reloading = true;
}

if Reloading = true &amp;&amp; Ammo != MaxAmmo{
    Ammo = 0;
    SecondsReloading = SecondsReloading + 1 / room_speed;
    
    if SecondsReloading &gt; SecondsUntilReloaded {
        Ammo = MaxAmmo;
        SecondsReloading = 0; 
        Reloading = false;
    }
    
}
</string>
