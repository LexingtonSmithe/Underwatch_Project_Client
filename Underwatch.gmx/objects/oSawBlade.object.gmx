    if collision_line(x,y,x+check_x,y,oSolid,true,1)    {
        change_hspd = true;
    }
    
    if change_vspd{  //bouncing off the horizontal side of the wall
        vspeed *= -1
        Bounces ++;
    }
    
    if change_hspd{//vertical side
        hspeed *= -1
        Bounces ++;
    }
}

image_angle += 12

CatchFire();

part_emitter_region(Sparks,emitter1,x+6,x-6,y+6,y-6,ps_shape_ellipse,0);
part_emitter_burst(Sparks,emitter1,particle1,5);



if Bounces &gt; 4 {
    part_system_destroy(Sparks);
    instance_destroy();
}
